{
  "name": "cpa-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"{,!(.next)/**/}*.{js, jsx}\"",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "update": "yarn upgrade --latest",
    "dep": "npx depcheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.10",
    "@mui/system": "^5.11.9",
    "@mui/x-date-pickers": "^5.0.20",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "dayjs": "^1.11.7",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.0",
    "eslint-config-prettier": "^8.6.0",
    "iron-session": "^6.3.1",
    "jest": "^29.4.3",
    "mongodb": "^5.1.0",
    "next": "13.2.0",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.2.0",
    "react-is": "18.2.0",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  },
  "devDependencies": {
    "husky": "^8.0.3"
  }
}
